{
    "name": "igorsgm/ssh-vault",
    "description": "üóùÔ∏è ‚Ä¢ Lightweight utility for easy SSH management. Add, list, access and remove connections with simple commands, keeping your SSH config organized.",
    "keywords": [
        "ssh",
        "ssh-client",
        "ssh-server",
        "laravel",
        "laravel-zero",
        "connection",
        "console",
        "cli"
    ],
    "homepage": "https://github.com/igorsgm/ssh-vault",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/igorsgm/ssh-vault/issues",
        "source": "https://github.com/igorsgm/ssh-vault"
    },
    "authors": [
        {
            "name": "Igor Moraes",
            "email": "igor.sgm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laminas/laminas-text": "^2.11",
        "laravel-zero/framework": "^11.0",
        "laravel-zero/phar-updater": "^1.4",
        "nunomaduro/termwind": "^2.2",
        "igorsgm/laravel-git-hooks": "^1.1",
        "laravel/pint": "^1.18.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/larastan": "^2.9.9",
        "pestphp/pest": "^2.35|^3.0",
        "spatie/invade": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Igorsgm\\SshVault\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/ssh-vault"]
}
